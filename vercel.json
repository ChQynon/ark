{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook(.*)",
      "dest": "dist/index.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/api/health",
      "dest": "dist/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "VERCEL": "1"
  },
  "github": {
    "silent": true
  }
} 